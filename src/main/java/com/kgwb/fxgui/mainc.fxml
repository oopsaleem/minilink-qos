                <TableView fx:id="personTable" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                  <columns>
                    <TableColumn fx:id="nameCol" maxWidth="100" prefWidth="158.0" text="C1" />
                    <TableColumn fx:id="surnameCol" minWidth="0.0" prefWidth="105.0" text="C2" />
